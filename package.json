{
  "name": "essentialrect-lib",
  "version": "1.1.5",
  "description": "javascript libraries for implementing essentialRect image display",
  "homepage": "https://github.com/mtsanford/essentialrect-lib",
  "main": "dist/EssentialRectLib.js",
  "exports": {
    ".": "./dist/EssentialRectLib.js",
    "./editor": "./dist/EssentialRectEditor.js"
  },
  "types": "dist/types/editor.d.ts",
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "webpack && npm run copy",
    "lint": "eslint . --ext .ts,.js,.jsx,.tsx",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "postversion": "git push && git push --tags",
    "version": "git add -A src",
    "copy": "copyfiles --flat ./src/*.css ./dist"
  },
  "keywords": [
    "essentialrect",
    "image",
    "javascript"
  ],
  "author": "Mark Sanford",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-loader": "^8.2.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.4.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-image-crop": "^9.0.4",
    "style-loader": "^3.3.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.8.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "optionalDependencies": {
    "react-image-crop": "^9.0.4"
  },
  "files": [
    "dist/**/*",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mtsanford/essentialrect-lib.git"
  }
}
